<%#= form_for(@user, :url => users_path(@user)) do |f| %>
  <%#= devise_error_messages! %>

<%= form_for("user", :url => users_path,  html: { :id => "newUser", :class => "span3" }, :remote => true, :format => :json) do |f| %>

    <div class="container">
      <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
          <h1 class="text-center login-title">REGISTAR USUARIO</h1>

          <div><%= f.label :email %><br />
          <%= f.email_field :email, :class => 'form-control' %></div>

          <div><%= f.label :password %><br />
          <%= f.password_field :password, :class => 'form-control' %></div>

          <div><%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, :class => 'form-control' %></div>

          <div><%= f.label :profile %><br />
          <%= f.password_field :profile, :class => 'form-control' %></div>
          
          <%= button_tag 'Guardar', type: 'button', id: 'save', class: 'btn btn-lg btn-primary btn-block' %>
          <div><%= f.submit "Guardar" %></div>
        
        </div>
      </div>
    </div>
<% end %>

<%= javascript_include_tag "tin_usuarios" %>


<%#= render "devise/shared/links" %>
